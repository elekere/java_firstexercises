public class Ekwunwu {
  public static void main(String[] arg) {
    System.out.println("I ga ekwunwu \"gbowam\" ka odaa ka egbe enu-igwe?");
      
    String firstName;
      String lastName;
       int awa, nkeji;         
  
       firstName = "Chukwuebuka "; // give firstName the value "Chukwuebuka"
      lastName = "Nzewi";/* give lastName the value "Nzewi" to remind Him of...
       * 29th of August when He wrote this programme*/
    awa = 11; // assign the value 11 to hour
      nkeji = 34; // set minute to 34 
  
  System.out.print(firstName);
   System.out.print (lastName);
   System.out.println (" bu afa mu. Abu mu onye agoziri agozi");
   System.out.print("Ihe na-aku ugbua bu ");
    System.out.print(awa);
    System.out.print(":");
       System.out.print(nkeji);
      System.out.println(".");

System.out.print("Nkeji ole gara aga malite n'etiti-abali bu: ");
System.out.println(awa * 60 + nkeji);
  }
} 

