   public String checkYourself (String userInput) {
   
      String = "miss";
      int index = locationCells.IndexOf(userInput);
      
      if (index >= 0) {
         
         locationCells.remove(index);
      
      if (locationCell.isEmpty() {
         
         result = "kill";
         
      
