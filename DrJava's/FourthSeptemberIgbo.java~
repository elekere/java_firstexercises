import java.util.Scanner;
public class FourthSeptember {
  public static void main (String[] args)
  { String udo;
    Scanner in = new Scanner(System.in);
    System.out.print("type something: ");
    udo = in.nextLine ();
    System.out.println ("Ife I typunyere bu: " + udo);
    System.out.print ("Typunye ife");
    udo = in.nextLine ();
    System.out.println ("Ife I typunyere bu: " + udo);
  }
}