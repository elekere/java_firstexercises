public class PrintTimeGgnz {

public static void timeFormat (int hour, int min) { 
System.out.print (hour);
System.out.print (":");
System.out.println (min);

}

public static void main (String [] args) {
int hour = 3;
int min = 59;
timeFormat (hour - 1, min);
}
}