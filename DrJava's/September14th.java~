import java.util.Scanner;

public class September14th {
  public static void main (String[] args) {
  
    int ozo = 0;
     System.out.println ("Vertically, ozo is displayed thus: "); 
     do {
        System.out.println (ozo); ozo++;
        
    } while (ozo <= 20);

System.out.println ();
    int nze = -20;
     System.out.println ("Vertically, nze is displayed thus: "); 
     
     do {
        System.out.println (nze); nze++;
        
    } while (nze = -20);
    
 System.out.println ();  
  
    int[] iruowelle = {1,2,3,4,5,6,7};
    System.out.println ("The sixth index of iruowelle is " + iruowelle [6]);
  }
  }
  